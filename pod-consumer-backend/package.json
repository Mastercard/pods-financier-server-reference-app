{
  "name": "pod-consumer-backend",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8081",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "get-random-values": "^1.2.0",
    "ip": "^1.1.5",
    "jsrsasign": "^8.0.15",
    "moment-timezone": "^0.5.35",
    "pod-smartphone-api": "0.1.0"
  },
  "scripts": {
    "dev": "nodemon app.js",
    "lint": "eslint .",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "es6": true,
      "node": true,
      "jest/globals": true
    },
    "extends": "eslint:recommended",
    "plugins": [
      "jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.8.2",
    "jest": "^25.4.0",
    "nodemon": "^2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  }
}
